{
    "name": "tlisp-vscode-support",
    "displayName": "Typed LISP VSCode support",
    "description": "Language support for Typed LISP",
    "repository": {
        "type": "github",
        "url": "https://github.com/Lukz127/TLISP-VSCode-support"
    },
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.98.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tlisp",
                "aliases": [
                    "Typed LISP",
                    "tlisp"
                ],
                "extensions": [
                    ".tlisp"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tlisp",
                "scopeName": "source.tlisp",
                "path": "./syntaxes/tlisp.tmLanguage.json"
            }
        ]
    },
    "dependencies": {
        "@vscode/vsce": "^3.3.2",
        "vsce": "^2.15.0"
    }
}